#Use 22501 as the default port
export SOS_CMD_PORT=22501
#Set the path variables needed for SOS
export SOS_WORK=`pwd`
export WORK_DIR=`pwd`
export SOS_DISCOVERY_DIR=`pwd`
export SOS_EVPATH_MEETUP=$SOS_DISCOVERY_DIR

#Set TAU to not be versbose
unset TAU_VERBOSE


#Make the database in memory as the default
export SOS_IN_MEMORY_DATABASE=TRUE
export SOS_EXPORT_DB_AT_EXIT=VERBOSE

#Declares that the envorinmental variables have been set
export SOS_ENV_SET=1

